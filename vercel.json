{
  "version": 2,
  "builds": [
    {
      "src": "api/vehiculo/placa.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/vehiculo/(.+)",
      "dest": "/api/vehiculo/placa.js"
    },
    {
      "src": "/api/taller/(.+)",
      "dest": "/api/taller/ciudad.js"
    }
  ]
}